{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base",
    "schedule:daily",
    "group:allNonMajor",
    ":semanticCommits",
    ":semanticCommitTypeAll(chore)",
    ":disableDependencyDashboard"
  ],
  "labels": ["dependencies", "renovate"],
  "baseBranches": ["main"],
  "rangeStrategy": "bump"
}
